{
  "name": "frenchbench-api",
  "version": "1.0.0",
  "description": "frenchbench api",
  "main": "index.js",
  "scripts": {
    "clean": "rm -rf dist tsconfig.tsbuildinfo",
    "build:ts": "tsc",
    "build": "npm run clean && npm run build:ts",
    "lint": "eslint ./src",
    "start:dev": "ts-node ./src/index.js",
    "start": "node ./dist/index.js",
    "db:upgrade": "knex migrate:latest --env production",
    "db:upgrade:staging": "knex migrate:latest --env staging",
    "db:upgrade:dev": "knex migrate:latest",
    "db:upgrade:next": "knex migrate:up --env production",
    "db:upgrade:next:staging": "knex migrate:up --env staging",
    "db:upgrade:next:dev": "knex migrate:up",
    "db:rollback": "knex migrate:rollback --all --env production",
    "db:rollback:staging": "knex migrate:rollback --all --env staging",
    "db:rollback:dev": "knex migrate:rollback --all",
    "db:rollback:last": "knex migrate:down --env production",
    "db:rollback:last:staging": "knex migrate:down --env staging",
    "db:rollback:last:dev": "knex migrate:down",
    "db:seed:dev": "knex seed:run",
    "test": "echo \"Error: no test specified\" && exit 1"
  },
  "author": "",
  "license": "ISC",
  "dependencies": {
    "axios": "^0.19.2",
    "bcrypt": "^5.0.0",
    "cookie": "^0.4.1",
    "cors": "^2.8.5",
    "dataloader": "^2.0.0",
    "date-fns": "^2.15.0",
    "dotenv": "^8.2.0",
    "express": "^4.17.1",
    "express-graphql": "^0.11.0",
    "frenchbench-common": "^1.0.0",
    "graphql": "^15.3.0",
    "jsonwebtoken": "^8.5.1",
    "knex": "^0.21.2",
    "morgan": "^1.10.0",
    "pg": "^8.3.0",
    "response-time": "^2.3.2",
    "sqlite3": "^5.0.0",
    "uuid": "^8.3.0"
  },
  "jest": {
    "testEnvironment": "node",
    "coveragePathIgnorePatterns": [
      "/node_modules/",
      "/migrations/",
      "/seeds/"
    ],
    "globalSetup": "./jestSetup.js"
  },
  "devDependencies": {
    "@types/express": "^4.17.7",
    "@types/node": "^14.0.27",
    "@typescript-eslint/eslint-plugin": "^3.8.0",
    "@typescript-eslint/parser": "^3.8.0",
    "eslint": "^7.6.0",
    "faker": "^4.1.0",
    "jest": "^26.2.2",
    "supertest": "^4.0.2",
    "ts-node": "^8.10.2",
    "typescript": "^3.9.7"
  }
}
